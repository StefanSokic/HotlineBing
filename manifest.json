{
  "manifest_version": 2,

  "name": "Hotline Bing",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "chrome_url_overrides" : {
    "newtab": "myPage.html"
  },
  "permissions": [
    "webRequest",
    "\u003Call_urls\u003E",
    "webRequestBlocking"
  ],

  "background": {
    "scripts": ["background.js"]
  }
}
